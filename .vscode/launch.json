  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "preLaunchTask": "launch-build",
      "program": "${workspaceFolder}\\buildAndReleaseTask\\index.js",
      "env": {
        "INPUT_TARGETFILEPATH": "SampleServiceConfiguration.cscfg",
        "foo.blank": "blank-override",
        "FOO_BAR": "bar-override"
      },
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
    }
  ]
}